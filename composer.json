{
    "name": "awesome-squiggle/wordpress-plugin",
    "description": "WordPress Block Plugin that extends the core WordPress Separator block",
    "type": "wordpress-plugin",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "wp-coding-standards/wpcs": "^3.0",
        "phpcompatibility/phpcompatibility-wp": "^2.1"
    },
    "scripts": {
        "phpcs": "phpcs --standard=WordPress awesome-squiggle.php",
        "phpcbf": "phpcbf --standard=WordPress awesome-squiggle.php"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}